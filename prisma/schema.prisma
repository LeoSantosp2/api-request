generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users {
  id         String   @id
  first_name String
  last_name  String
  email      String   @unique
  password   String
  token_auth String?  @db.VarChar(255)
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
